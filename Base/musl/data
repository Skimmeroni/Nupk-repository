#!/bin/sh -e

DEPENDENCIES=""
INFO="Light, fast and simple C library focused on standards-conformance and safety"
PRETTY_NAME="musl"
SOURCES="https://www.musl-libc.org/releases/musl-$VERSION.tar.gz"
SOURCES="$SOURCES https://codeberg.org/kiss-community/repo/raw/branch/master/core/musl/files/getconf.c"
SOURCES="$SOURCES https://codeberg.org/kiss-community/repo/raw/branch/master/core/musl/files/getent.c"
SOURCES="$SOURCES https://codeberg.org/kiss-community/repo/raw/branch/master/core/musl/files/cdefs.h"
SOURCES="$SOURCES https://codeberg.org/kiss-community/repo/raw/branch/master/core/musl/files/queue.h"
SOURCES="$SOURCES https://codeberg.org/kiss-community/repo/raw/branch/master/core/musl/files/tree.h"
MAJOR="1"
MINOR="2"
PATCH="5"
VERSION="$MAJOR.$MINOR.$PATCH"
